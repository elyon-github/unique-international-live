<odoo>
	<record id="view_stock_picking_tree_inherit" model="ir.ui.view">
		<field name="name">return details fields</field>
		<field name="model">stock.picking</field>
		<field name="inherit_id" ref="stock.view_picking_form"/>
		<field name="arch" type="xml">
			<xpath expr="//group[@name='other_infos']" position='after'>
				<group name="e_comm" string="Other Details">
					<field name="sales_team_id" string="Sales Team"/>
					<field name="analytic_account_id" string="Analytic Account"/>
					<field name="sales_person_id" string="Salesperson"/>
				</group>
			</xpath>
		</field>
	</record>

	<record id="view_account_move_tree_inherit" model="ir.ui.view">
		<field name="name">dr only field</field>
		<field name="model">account.move</field>
		<field name="inherit_id" ref="account.view_move_form"/>
		<field name="arch" type="xml">
			<xpath expr="//field[@name='invoice_date']" position='after'>
				<field name="dr_only" string="DR Only"/>
			</xpath>
		</field>
	</record>
</odoo>